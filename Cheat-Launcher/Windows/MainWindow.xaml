<Window x:Class="Cheat_Launcher.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cheat_Launcher.Components"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Icon="pack://application:,,,/Resources/logo.ico"
        Title="Dolos" Height="600" Width="800">
    <Grid Background="Black">
        <StackPanel x:Name="HeaderContainer" VerticalAlignment="Top" Margin="0, 0, 0, 0">
        </StackPanel>
        <StackPanel x:Name="GamesListContainer" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0, 70, 0, 0">
        </StackPanel>
        <StackPanel x:Name="MainContainer" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="273, 55, 0, 0">
        </StackPanel>
    </Grid>
</Window>
